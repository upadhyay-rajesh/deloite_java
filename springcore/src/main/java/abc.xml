<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
http://www.springframework.org/schema/context
http://www.springframework.org/schema/context/spring-context-2.5.xsd">


	<bean id="m1" class="com.springcore.Trainer">
		<property name="cc1">
			<ref bean="m4" />
		</property>
	</bean>
	
	<bean id="m3" class="com.springcore.Liberary"></bean>
	
	<bean id="m4" class="com.springcore.Canteen"></bean>
	
	<bean id="m2" class="com.springcore.Trainees">
		<property name="ti"> <!-- setter method of ti -->
			<ref bean="m1" />  <!-- DI -->
		</property>
		
		<property name="lb">
			<ref bean="m3"/>
		</property>
		
		<property name="cc">
			<ref bean="m4" />
		</property>
	</bean>

	
</beans>














